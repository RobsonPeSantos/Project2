<link rel="stylesheet" href="/stylesheets/style.css" />
<div class="details">
    {{#with serie}}
    <div>
        <img src="{{image}}" alt="">
    </div>
    <h4>Title: {{title}}</h4>
    <p class="p">Cast : {{cast}}</p>
    <p class="pg"> Genre : {{genre}}</p>
    <span>Description : {{description}}</span>
    <p class="rate"><strong>Rate : {{rate}}</strong></p>
    <a href="/delete/serie/{{id}}">Deletar serie</a>
    {{/with}}
</div>